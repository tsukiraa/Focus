{
    "manifest_version": 2,
    "name": "Focus.",
    "description": "Focus is site blocking extension made in mind for usage of one specific person.",
    "version": "1.2.0",
    
        "content_scripts": [
          {
            "js": ["blocked_youtube.js"],
            "matches": [
              "https://www.youtube.com/*"
            ]
          },{
            "js": ["blocked_reddit.js"],
            "matches": [
              "https://www.reddit.com/*"
            ]
          },{
            "js": ["blocked_stream.js"],
            "matches": [
              "https://www.stream.cz/*"
            ]
          }]
          
}